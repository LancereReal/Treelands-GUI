-- Scripts:

local function RZOC_fake_script() -- MainFrame.KeyToggle 
	local script = Instance.new('LocalScript', MainFrame)

	--Open Script
	local uis = game:GetService("UserInputService")
	local ui = script.Parent.Parent.Parent.TreeLandsGUI
	local toggle = false
	uis.InputBegan:Connect(function(key)
		if key.KeyCode == Enum.KeyCode.RightShift then
				ui.Enabled = not ui.Enabled;
			end
		end)
	
	
end
coroutine.wrap(RZOC_fake_script)()
local function EEZM_fake_script() -- Exit.ToggleScript 
	local script = Instance.new('LocalScript', Exit)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.Visible = false
	end)
end
coroutine.wrap(EEZM_fake_script)()
local function PWNG_fake_script() -- Page1Toggle.LocalScript 
	local script = Instance.new('LocalScript', Page1Toggle)

	script.Parent.MouseButton1Down:Connect(function()
		script.Parent.Parent.Page1.Visible = true
		script.Parent.Parent.Page2.Visible = false
		script.Parent.Parent.Page3.Visible = false
		script.Parent.Parent.Page4.Visible = false
		script.Parent.Parent.Page5.Visible = false
	end)
end
coroutine.wrap(PWNG_fake_script)()
local function MFRB_fake_script() -- Page2Toggle.LocalScript 
	local script = Instance.new('LocalScript', Page2Toggle)

	script.Parent.MouseButton1Down:Connect(function()
		script.Parent.Parent.Page1.Visible = false
		script.Parent.Parent.Page2.Visible = true
		script.Parent.Parent.Page3.Visible = false
		script.Parent.Parent.Page4.Visible = false
		script.Parent.Parent.Page5.Visible = false
	end)
end
coroutine.wrap(MFRB_fake_script)()
